<Resource>
  <Folder name="GacGenConfig">
    <Folder name="Cpp">
      <Text name="SourceFolder">Source</Text>
      <Text name="NormalInclude">../../../../GacUI.h</Text>
      <Text name="ReflectionInclude">../../../../Reflection/TypeDescriptors/GuiReflectionPlugin.h</Text>
      <Text name="Name">GuiFakeDialogServiceUI</Text>
      <Text name="CppCompressed">GuiFakeDialogServiceUI.cpp</Text>
    </Folder>
  </Folder>

  <Folder name="Images">
    <Image name="None" content="File">Images/None.png</Image>
    <Image name="Error" content="File">Images/Error.png</Image>
    <Image name="Question" content="File">Images/Question.png</Image>
    <Image name="Warning" content="File">Images/Warning.png</Image>
