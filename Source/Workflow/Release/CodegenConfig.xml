<?xml version="1.0" encoding="utf-8" ?>
<codegen>
  <folders>
    <folder path="..\Source" />
    <folder path="..\Import" />
  </folders>
  <categories>
    <category name="vlpp" pattern="\Import\Vlpp."/>
    <category name="vlppos" pattern="\Import\VlppOS."/>
    <category name="vlppregex" pattern="\Import\VlppRegex."/>
    <category name="vlppreflection" pattern="\Import\VlppReflection."/>
    <category name="vlppparser2" pattern="\Import\VlppGlrParser."/>
    <category name="vlppparser2compiler" pattern="\Import\VlppGlrParserCompiler."/>
    <category name="compiler" pattern="\Source\">
      <except pattern="\Runtime\" />
      <except pattern="\Library\" />
    </category>
    <category name="runtime" pattern="\Source\Runtime\" />
    <category name="library" pattern="\Source\Library\" />
  </categories>
  <output path=".">
    <codepair category="vlpp" filename="Vlpp" generate="false"/>
    <codepair category="vlppos" filename="VlppOS" generate="false"/>
    <codepair category="vlppregex" filename="VlppRegex" generate="false"/>
    <codepair category="vlppreflection" filename="VlppReflection" generate="false"/>
    <codepair category="vlppparser2" filename="VlppGlrParser" generate="false"/>
    <codepair category="vlppparser2compiler" filename="VlppGlrParserCompiler" generate="false"/>
    <codepair category="library" filename="VlppWorkflowLibrary" generate="true"/>
    <codepair category="runtime" filename="VlppWorkflowRuntime" generate="true"/>
    <codepair category="compiler" filename="VlppWorkflowCompiler" generate="true"/>
  </output>
</codegen>